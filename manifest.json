{
  "manifest_version": 2,
  "name": "e-Devlet Soyağacı",
  "version": "0.1.3",
  "description": "e-Devlet kapısı üzerinden soyağacınızı şemaya (şecere) çevirin ve paylaşın.",
  "icons": {
    "32" : "icon32.png",
    "48" : "icon48.png"
  },
  "minimum_chrome_version": "39",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage" ]
}
